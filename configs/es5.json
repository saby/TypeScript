{
  "compilerOptions": {
    "alwaysStrict": true,
    "baseUrl": ".",
    "importHelpers": true,
    "lib": ["dom", "es5", "es2016", "es2018.promise", "scripthost"],
    "module": "amd",
    "moduleResolution": "classic",
    "noUnusedLocals": true,
    "paths": {
      "Core/*": ["WS.Core/core/*"],
      "Lib/*": ["WS.Core/lib/*"],
      "Transport/*": ["WS.Core/transport/*"]
    },
    "target": "es5",
    "typeRoots" : ["node_modules/@types"],
    "types" : ["requirejs"]
  }
}
